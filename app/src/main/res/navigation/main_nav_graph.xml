<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.vehiclemaintenancerebuild.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_addNewVehicleFragment"
            app:destination="@id/addNewVehicleFragment"
            app:enterAnim="@anim/slide_in_from_bottom" />
    </fragment>
    <fragment
        android:id="@+id/addNewVehicleFragment"
        android:name="com.example.vehiclemaintenancerebuild.myvehicles.AddNewVehicleDialogFragment"
        android:label="AddNewVehicleFragment" >
        <action
            android:id="@+id/action_addNewVehicleFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:exitAnim="@anim/slide_out_from_top" />
    </fragment>
</navigation>